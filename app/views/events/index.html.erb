<div class="container" id="calendar" data-group-id="<%= params[:group_id] %>" >
  <div class="row">
    <div class="col-md-1"></div>
    <!-- mid -->
    <div class="col-md-8">
      <div class="calendar"></div>

    </div>
    <!-- end mid -->
    <div class="col-md-1"></div>
  </div>
</div>

  <div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-8"><h1>HISTORY</h1></hr></div>
  </div>

  <div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-8">
      <% @eventsdone.each do |event| %>
       <p style="font-size: 12px" ><%= event.startDate %></p>
        <p style="color: #ffc107!important;font-size: 16px"><%= event.title %></p>
        <p><span style="font-weight:bold">Duration:</span> <%= event.duration %> hours</p>
        <p><span style="font-weight:bold">Speaker:</span><%= event.speaker %></p>
        <p style="font-size: 12px"><%= event.something %></p>
        <hr>
      <% end %>
    </div>
  </div>


<div id='remote_container'></div>